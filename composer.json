{
    "name": "oneapp/oneapp",
    "description": "Integrate our APIs to build a web or mobile integration to accept payments, make payment of Bills",
    "keywords": [
        "1app",
        "payments",
        "bills",
        "transfers",
        "money",
        "currency",
        "transfer",
        "reliable",
        "cheaper",
        "safer",
        "no-down-time",
        "payment-link"
    ],
    "type": "library",
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "illuminate/support": "~6|~7|~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "scrutinizer/ocular": "~1.1",
        "php-coveralls/php-coveralls": "^2.0",
        "mockery/mockery": "^1.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oneapp\\Oneapp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "OdevLAB",
            "email": "odevservices@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Oneapp\\Oneapp\\OneappServiceProvider"
            ],
            "aliases": {
                "Oneapp": "Oneapp\\Oneapp\\Facades\\Oneapp"
            }
        }
    }
}

